{
    "name": "github-api-v2",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "src/index.js",
    "scripts": {
        "compile": "tsc --watch",
        "start": "node --es-module-specifier-resolution node ./src",
        "test": "jest --config jest.config.js",
        "rm-js": "find . -name '*.js' -type f -delete",
        "rm-declarations": "find . -name '*.d.ts' -type f -delete",
        "rm-declaration-maps": "find . -name '*.d.ts.map' -type f -delete",
        "rm-maps": "find . -name '*.js.map' -type f -delete",
        "clean": "npm run rm-js && npm run rm-declarations && npm run rm-declaration-maps && npm run rm-maps && npm run rm-js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cloud-hybrid/github-api-v2.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cloud-hybrid/github-api-v2/issues"
    },
    "homepage": "https://github.com/cloud-hybrid/github-api-v2#readme",
    "dependencies": {
        "@octokit/plugin-paginate-rest": "^2.17.0",
        "@octokit/rest": "latest"
    },
    "devDependencies": {
        "@aws-sdk/types": "latest",
        "@cloud-technology/secrets-manager": "latest",
        "@jest/globals": "latest",
        "@octokit/types": "latest",
        "@types/inquirer": "latest",
        "@types/jest": "latest",
        "@types/node": "latest",
        "@types/uuid": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/experimental-utils": "latest",
        "@typescript-eslint/parser": "latest",
        "@typescript-eslint/scope-manager": "latest",
        "@typescript-eslint/type-utils": "latest",
        "@typescript-eslint/typescript-estree": "latest",
        "@typescript-eslint/utils": "latest",
        "eslint": "latest",
        "jest": "latest",
        "node-notifier": "latest",
        "ts-jest": "latest",
        "ts-node": "latest",
        "tslib": "latest",
        "typescript": "latest",
        "typescript-eslint-language-service": "latest"
    }
}
